<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="160.0" prefWidth="289.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <HBox alignment="TOP_CENTER" prefWidth="200.0">
         <children>
            <TextField fx:id="rootField" editable="false" focusTraversable="false" minWidth="-Infinity" prefHeight="25.0" text="Root" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="TOP_CENTER" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
         <children>
            <Button fx:id="prevDayBtn" mnemonicParsing="false" text="&lt;&lt;" />
            <TextField fx:id="dayField" alignment="CENTER" editable="false" focusTraversable="false" text="Date">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="nextDayBtn" mnemonicParsing="false" text="&gt;&gt;" />
         </children>
      </HBox>
      <VBox fx:id="foldersVBox" prefHeight="200.0" prefWidth="100.0" />
      <Button fx:id="createBtn" mnemonicParsing="false" onAction="#createFolders" text="Create Folders" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
